{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "powerline",
                    "leading_diamond": "\ue0c7\u2593",
                    "foreground": "lightGreen",
                    "properties": {
                        "windows": "\ue70f",
                        "prefix": "",
                        "postfix": "",
                        "wsl": "",
                        "wsl_separator": ""
                    }
                },
                {
                    "type": "session",
                    "style": "plain",
                    "foreground": "lightGreen",
                    "properties": {
                        "postfix": " =>"
                    }
                },
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "lightYellow",
                    "properties": {
                        "style": "full"
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "lightBlue",
                    "properties": {
                        "branch_icon": " <lightYellow><b>î‚  </b></>",
                        "display_stash_count": true,
                        "working_color": "lightYellow",
                        "display_upstream_icon": true,
                        "status_colors_enabled": true,
                        "display_status": true,
                        "stash_count_icon": "\uF692 ",
                        "prefix": "<lightYellow>( on </>",
                        "postfix": " <lightYellow>)</>"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#68a063",
                    "properties": {
                        "display_version": true,
                        "prefix": " ",
                        "postfix": "",
                        "display_mode": "files",
                        "display_package_manager": true,
                        "yarn_icon": "/yarn",
                        "npm_icon": "/npm"
                    }
                },
                {
                    "type": "crystal",
                    "style": "plain",
                    "foreground": "#4063D8",
                    "properties": {
                        "display_version": true,
                        "display_mode": "files",
                        "prefix": " ",
                        "postfix": ""
                    }
                },
                {
                    "type": "ruby",
                    "style": "plain",
                    "foreground": "#DE3F24",
                    "properties": {
                        "display_version": true,
                        "prefix": " ",
                        "postfix": "",
                        "display_mode": "files"
                    }
                },
                {
                    "type": "python",
                    "style": "plain",
                    "foreground": "#FED142",
                    "properties": {
                        "display_virtual_env": false,
                        "display_version": true,
                        "prefix": " ",
                        "postfix": "",
                        "display_mode": "context"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "exit",
                    "style": "powerline",
                    "foreground": "cyan",
                    "properties": {
                        "display_exit_code": false,
                        "always_enabled": true,
                        "error_color": "lightRed",
                        "prefix": "\u276F"
                    }
                }
            ]
        }
    ]
}
